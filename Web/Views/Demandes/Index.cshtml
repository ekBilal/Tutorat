@model IEnumerable<Models.Demande>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.Cours)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Etudiant)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Commentaire)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Etat)
		</th>
		<th></th>
	</tr>

@foreach (var item in Model) {
	<tr>
		<td>
			@Html.DisplayFor(modelItem => item.Cours.Nom)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Etudiant.Nom)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Commentaire)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Etat)
		</td>
		<td>
			@Html.ActionLink("Edit", "Edit", new { id = item.IdDemande }) |
			@Html.ActionLink("Details", "Details", new { id = item.IdDemande })
		</td>
	</tr>
}

</table>
