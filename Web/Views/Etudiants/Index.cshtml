@model IEnumerable<Models.Etudiant>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PSR)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prenom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PSR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prenom)
        </td>

		<td>
			@Html.ActionLink("Details", "Details", new { id = item.IdEtudiant }) |
			@Html.ActionLink("Edit", "Edit", new { id = item.IdEtudiant }) |
			@Html.ActionLink("Delete", "Delete", new { id = item.IdEtudiant })
		</td>
    </tr>
}

</table>
